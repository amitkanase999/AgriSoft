<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.Fertilizer.hibernate.SaveDailyStock" table="save_daily_stock">

		<id name="pkSaveDailyStock" type="java.lang.Long" column="pk_daily_stock_id">
			<generator class="increment" />
		</id>


		<property name="productName" type="java.lang.String" column="product_name" />

		<property name="batchNo" type="java.lang.String" column="batch_number" />

		<property name="companyName" type="java.lang.String" column="company" />


		<property name="weight" type="java.lang.Double" column="weight" />

		<property name="quantity" type="java.lang.Double" column="dsquantity" />


		<property name="insertDate" type="java.lang.String" column="insert_date" />
		
		<property name="productCategory" type="java.lang.String" column="product_category" />
		
		<property name="expdate" type="java.util.Date" column="expdate" />
		
		<property name="supplier_name" type="java.lang.String" column="supplier_name" />

	</class>
</hibernate-mapping>